{
  "version": "1.0",
  "defaultProvider": "unpkg",
  "libraries": [
    {
      "library": "bootstrap@4.6.1",
      "destination": "wwwroot/lib/bootstrap/",
      "files": [
        "scss/**/*",
        "dist/**/*"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "jquery-loading-overlay@2.1.7",
      "destination": "wwwroot/lib/jquery-loading-overlay/",
      "files": [
        "loadingoverlay.js",
        "loadingoverlay.min.js",
        "loadingoverlay.min.js.map"
      ]
    }
  ]
}